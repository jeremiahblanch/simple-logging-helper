{
  "name": "simple-logging-helper",
  "version": "1.0.4",
  "description": "a helper tool for logging javascript in a browser",
  "source": "src/index.js",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "parcel build",
    "build-publish": "npm run build && npm run publish-to-npm",
    "publish-to-npm": "npm publish --access public",
