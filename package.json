  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {}
  },
  "keywords": [
    "log",
    "logging"
  ],
  "author": "Jeremiah Blanch",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.12.0"
  }
}
